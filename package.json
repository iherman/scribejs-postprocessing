{
  "name": "scribejs-resolutions",
  "version": "1.0.0",
  "description": "Gathering resolutions from minutes generated by scribejs",
  "main": "main.js",
  "scripts": {
    "github": "ts-node src/main.ts",
    "local": "ts-node src/main.ts --local",
    "docs": "./node_modules/typedoc/bin/typedoc",
    "browser": "(cd browser_view; tsc --target ES2019 resolution_view.ts)",
    "dist": "tsc; (cd browser_view; tsc --target ES2019 resolution_view.ts) ; ./node_modules/typedoc/bin/typedoc"
  },
  "author": {
    "name": "Ivan Herman",
    "email": "ivan@w3.org",
    "url": "https://www.w3.org/People/Ivan/"
  },
  "engines": {
    "node": "12x",
    "npm": "6x"
  },
  "keywords": [
    "W3C",
    "scribe",
    "minute taking"
  ],
  "license": "W3C-20150513",
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/showdown": "^1.9.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "typedoc": "^0.19.1",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "octocat": "^1.2.1",
    "showdown": "^1.9.1"
  }
}
